package {	class Mas {		public function Mas():void {			setRandomSeed(Math.random()*10000);		}				public function abs(val:Number):Number {			return val>0?val:-val;		}				public function ceil(val:Number):Number {			return val>0?++val>>0:val>>0;		}				public function floor(val:Number):Number {			return val>0?val>>0:--val>>0;		}		public function max(...rest):Number {			var len:int = rest.length;			if(len == 2){				return rest[0] < rest[1] ? rest[1] : rest[0];			}else{				var temp:Number = rest[0];				for (var i:int = 1; i < len; i++) {					temp = temp < rest[i] ? rest[i] : temp;				}						return temp;			}		}				public function min(...rest):Number {			var len:int = rest.length;			if(len == 2){				return rest[0] > rest[1] ? rest[1] : rest[0];			}else{				var temp:Number = rest[0];				for (var i:int = 1; i < len; i++) {					temp = temp > rest[i] ? rest[i] : temp;				}						return temp;			}		}		public var randomSeed:Number = 1;		public function setRandomSeed(seed:Number,count:int = 100):void{			randomSeed = seed;			for (var i:int = 0; i < count; i++) {				random();			}		}		public function random():Number{			randomSeed = (randomSeed*1664525+1013904223)%0x100000000;			return randomSeed/0x100000000;		}		public function randomNum():Number{			return randomSeed/0x100000000;		}				public function round(val:Number):Number {			val+=0.5;			return val>0?val>>0:--val>>0;		}	}}